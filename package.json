{
  "name": "feelfree",
  "version": "1.0.0",
  "private": true,
  "description": "feel-free.tirol paragliding school Westendorf",
  "author": "Daniel Naschberger",
  "precommit": "npm run lint",
  "scripts": {
    "current-git-sha": "set NUXT_ENV_CURRENT_GIT_SHA=`git rev-parse --short HEAD`",
    "deploy-env:gh-pages": "set DEPLOY_ENV=GH_PAGES",
    "dev": "npm run current-git-sha && nuxt",
    "build": "npm run current-git-sha && nuxt build",
    "build:gh-pages": "npm run current-git-sha && npm run deploy-env:gh-pages && npm run build",
    "start": "nuxt start",
    "generate": "npm run current-git-sha nuxt generate",
    "generate:gh-pages": "npm run current-git-sha && npm run deploy-env:gh-pages && npm run generate",
    "lint:js": "eslint --fix  --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "npm run lint:js"
  },
  "dependencies": {
    "@nuxt/content": "^1.14.0",
    "@nuxtjs/composition-api": "^0.24.0",
    "@vue/composition-api": "^1.0.0-rc.9",
    "core-js": "^3.9.1",
    "nuxt": "^2.15.3"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^6.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/tailwindcss": "^4.0.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.7.0",
    "postcss": "^8.2.8",
    "prettier": "^2.2.1"
  }
}
